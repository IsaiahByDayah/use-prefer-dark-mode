{
    "printWidth": 120,
    "tabWidth": 4,
    "semi": false,
    "singleQuote": false,
    "trailingComma": "es5",
    "bracketSpacing": true,
    "useTabs": false,
    "proseWrap": "preserve",
    "arrowParens": "avoid",
    "parser": "typescript",
    "overrides": [
        {
            "files": "*.json",
            "options": {
                "parser": "json"
            }
        }
    ]
}
